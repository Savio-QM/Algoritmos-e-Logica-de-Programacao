Algoritmo "Exercício de Fixação"
Var
   produto1 : caractere
   produto2 : caractere
   preco1 : real
   preco2 : real
   idade : inteiro
   codigo : inteiro
   genero : caractere


Inicio
      //Definição de cada variável
      produto1 <- "Computador"
      produto2 <- "TV"
      preco1 <- 2100.5
      preco2 <- 1830.0
      idade <- 30
      codigo <- 5291
      genero <- "F"
      
      //Escrever as frases comm base nas variaveis
      escreval ("Produtos: ")
      escreval ("O produto ", produto1, " custa R$", preco1)
      escreval ("O produto ", produto2, " custa R$", preco2)
      escreval
      escreval ("Código =", codigo)
      escreval ("Dados da pessoa: genero ", genero, " e idade", idade)
Fimalgoritmo
